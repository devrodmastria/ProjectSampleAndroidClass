<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.wmschane.projectsample.TempFragment"
    android:baselineAligned="false">

    <!-- TODO: Update blank fragment layout -->





    <Button
        android:text="Date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button6"
        android:layout_weight="1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="212dp" />

    <TextView
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/TempDegree"
        android:layout_alignParentStart="true"
        android:layout_marginStart="504dp"
        android:layout_marginTop="54dp"
        android:id="@+id/textView6" />

    <Button
        android:text="-"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/TempMinus"
        android:layout_alignBottom="@+id/button6"
        android:layout_toStartOf="@+id/button6"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="70dp" />

    <Button
        android:text="+"
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/TempPlus"
        android:layout_alignBaseline="@+id/TempMinus"
        android:layout_alignBottom="@+id/TempMinus"
        android:layout_toEndOf="@+id/button6" />

    <EditText
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="3"
        android:id="@+id/TempDegree"
        android:singleLine="false"
        android:text="75"
        android:layout_width="wrap_content"
        android:layout_alignBottom="@+id/TempPlus"
        android:layout_centerHorizontal="true"
        android:textAlignment="center" />

</RelativeLayout>
